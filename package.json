{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "./src/server/server.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run prettier:staged",
      "npm run eslint:fix",
      "npm run eslint"
    ]
  },
  "scripts": {
    "prettier:staged": "npx pretty-quick --staged",
    "prettier": "npx prettier --write ./src/**/*.ts",
    "eslint": "eslint ./src/**/*.ts --max-warnings=0",
    "eslint:fix": "eslint ./src/**/*.ts --fix",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:coverage": "cross-env NODE_ENV=test vitest --coverage",
    "test:watch": "cross-env NODE_ENV=test vitest watch",
    "dev": "cross-env NODE_ENV=developer ts-node-dev ./src/index.ts",
    "prepare": "husky install",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "@vitest/coverage-c8": "^0.25.5",
    "cross-env": "^7.0.3",
    "dotenv": "16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "mongodb-memory-server": "8.10.1",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.3.1",
    "typescript": "^4.9.3",
    "vitest": "0.25.5"
  },
  "dependencies": {
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "mongoose": "6.7.5",
    "ts-node-dev": "^2.0.0",
    "yup": "0.32.11"
  }
}
